{
  "mcpServers": {
    "timezone": {
      "command": "python",
      "args": [
        "-m",
        "mcp_server_time",
        "--local-timezone=America/New_York"
      ]
    },
    "weather": {
      "command": "python",
      "args": [
        "./weather-server-python/src/weather/server.py"
      ],
      "enabled": true
    },
    "sequentialthinking": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "mcp/sequentialthinking"
      ],
      "enabled": true
    },
    "memory": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/memory"
      ],
      "env": null,
      "enabled": true
    },
    "fetch": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/fetch"
      ],
      "env": null,
      "enabled": true
    },
    "npx-service": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "npx-service",
        "https://github.com/cbruyndoncx/mcp-rtfm.git"
      ],
      "env": null,
      "enabled": true
    }
  }
}
